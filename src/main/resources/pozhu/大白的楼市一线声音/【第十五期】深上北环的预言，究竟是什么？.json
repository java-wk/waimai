u697c\u5e02\uff1f\u6211\u4eec\u5e94\u8be5\u600e\u4e48\u5224\u65ad\u548c\u9009\u62e9\uff1f\u6b27\u795e\u7684\u770b\u6cd5\u662f\u5929\u6d25\u76ee\u524d\u662f\u6709\u5927\u5229\u597d\u7684\u3002\u4f46\u5982\u679c\u4f60\u63a5\u76d8\u4ee5\u540e\u5c31\u5fc5\u987b\u6709\u6301\u7eed\u7684\u5229\u597d\uff0c\u4eba\u53e3\u80fd\u4e0d\u80fd\u8f6c\u5316\u4e3a\u751f\u4ea7\u529b\uff0c\u80fd\u4e0d\u80fd\u6709\u6548\u7684\u8f6c\u6362\u6210\u7eb3\u7a0e\u4eba\uff0c\u80fd\u4e0d\u80fd\u5e26\u9886\u5b83\u7684\u4ea7\u4e1a\u5347\u7ea7\uff0c\u79d1\u6280\u8ddf\u4e92\u8054\u7f51\u9769\u547d\u3002\u4ee5\u5929\u6d25\u4e00\u8d2f\u7684\u53e3\u7891\uff0c\u5176\u5b9e\u4e0d\u600e\u4e48\u770b\u597d\u3002\u6768\u7ea2\u65ed\u7684\u89c2\u70b9\u662f\u5929\u6d25\u4e00\u76f4\u5b9a\u4f4d\u86ee\u9ad8\u7684\u6210\u4e3a\u5317\u65b9\u7684\u7ecf\u6d4e\u4e2d\u5fc3\u3002\u4f46\u53d1\u5c55\u7684\u4e0d\u5982\u4eba\u610f\uff0c\u6240\u4ee5\u8fd9\u4e2a\u57ce\u5e02\u7ecf\u6d4e\u4ea7\u4e1a\u548c\u91d1\u878d\u51fa\u73b0\u4e00\u4e9b\u95ee\u9898\uff0c\u5305\u62ec\u524d\u6bb5\u65f6\u95f4\u5929\u4fdd\u96c6\u56e2\u7206\u51fa\u503a\u52a1\u521a\u5151\u5371\u673a\uff0c\u8fd9\u4e2a\u5e94\u8be5\u662f\u8fc7\u53bb\u5927\u624b\u7b14\u7684\u56fd\u5bb6\u6295\u8d44\u548c\u4ea7\u51fa\u4e0d\u6210\u6b63\u6bd4\uff0c\u6295\u8d44\u86ee\u591a\u7684\uff0c\u4f46\u662f\u6295\u8d44\u7684\u6548\u76ca\u504f\u4f4e\uff0c\u4ea7\u51fa\u4e0d\u591a\uff0c\u8fd9\u4e2a\u77db\u76fe\u79ef\u84c4\u5230\u76ee\u524d\uff0c\u5bfc\u81f4\u6574\u4e2a\u5b8f\u89c2\u6570\u636e\u8868\u73b0\u504f\u5dee\u3002\u4eba\u624d\u65b0\u653f\u53ea\u662f\u4e00\u4e2a\u8109\u51b2\u884c\u60c5\uff0c\u5929\u6d25\u7684\u623f\u4ef7\u8dcc\u4e86\u4e00\u4e9b\uff0c\u4f46\u662f\u73b0\u5728\u770b\u4e0d\u5230\u5e95\u90e8\uff0c\u8109\u51b2\u884c\u60c5\u4e0d\u80fd\u6539\u53d8\u5e02\u573a\u8fd0\u884c\u5927\u7684\u65b9\u5411\uff0c\u81ea\u4f4f\u53ef\u4ee5\u4e70\uff0c\u4f46\u662f\u6295\u8d44\u8fd8\u9700\u89c2\u5bdf\u3002\u7b2c\u4e94\u3001\u6d77\u5357\u9650\u8d2d\u4ee5\u540e\uff0c\u56fd\u5185\u65c5\u6e38\u5730\u4ea7\u8fd8\u6709\u6295\u8d44\u673a\u4f1a\u5417\uff1f\u5728\u8fd9\u4e2a\u95ee\u9898\u4e0a\u6b27\u795e\u548c\u6768\u7ea2\u65ed\u7684\u770b\u6cd5\u662f\u76f8\u53cd\u7684\u3002\u6b27\u795e\u8ba4\u4e3a\u8bf4\u5168\u56fd\u7684\u65c5\u6e38\u5730\u4ea7\u90fd\u6ca1\u6709\u4ef7\u503c\uff0c\u56e0\u4e3a\u4e00\u4e2a\u5730\u65b9\u8981\u6301\u7eed\u5347\u503c\uff0c\u5fc5\u987b\u6709\u6301\u7eed\u7684\u70ed\u5ea6\u7092\u4f5c\uff0c\u65c5\u6e38\u5730\u4ea7\u5f80\u5f80\u662f\u4e00\u6ce2\uff0c\u53ea\u8981\u4ed6\u628a\u8fd9\u4e2a\u9879\u76ee\u5356\u7ed9\u4f60\uff0c\u5356\u5b8c\u4ee5\u540e\u5c31\u64a4\u573a\u4e86\uff0c\u5f00\u53d1\u5546\u6ca1\u6709\u540e\u7eed\u8ddf\u8fdb\u548c\u70d8\u6258\uff0c\u4f60\u5f88\u5bb9\u6613\u6210\u4e3a\u6700\u540e\u4e00\u68d2\u3002\u4f46\u6768\u7ea2\u65ed\u4e3e\u4e86\u4e2a\u4f8b\u5b50\u8bf4\u7f8e\u56fd\u6700\u8fd1\u8fd9\u51e0\u5e74\u6da8\u7684\u6700\u5389\u5bb3\u7684\u662f\u897f\u6d77\u5cb8\u4e00\u4e9b\u57ce\u5e02\uff0c\u50cf\u8fc8\u963f\u5bc6\uff0c\u9002\u5408\u5ea6\u5047\u7684\u57ce\u5e02\uff0c\u751a\u81f3\u6bd4\u7ebd\u7ea6\u90fd\u6da8\u5f97\u591a\u3002\u8fd9\u4e2a\u4f8b\u5b50\u53ef\u4ee5\u6620\u5c04\u5230\u4e2d\u56fd\u7684\u60c5\u51b5\uff0c\u65c5\u6e38\u5730\u4ea7\u7684\u7279\u5f81\u662f\uff0c\u5b83\u53ef\u80fd\u4f1a\u5b58\u5728\u597d\u51e0\u5e74\uff0c\u4f46\u7a81\u7136\u5927\u98ce\u4e00\u6765\uff0c\u4e00\u6ce2\u5927\u6da8\u3002\u6240\u4ee5\u6d77\u5357\u4e5f\u597d\uff0c\u4e91\u5357\u4e5f\u597d\uff0c\u5982\u679c\u65f6\u673a\u628a\u63e1\u597d\u662f\u53ef\u4ee5\u6295\u7684\uff0c\u4e0d\u662f\u8bf4\u4e00\u68d2\u6253\u6b7b\u3002\u5728\u8fd9\u4e00\u70b9\uff0c\u7c73\u5b85\u7684\u89c2\u70b9\u662f\u66f4\u503e\u5411\u6768\u7ea2\u65ed\u7684\u3002\u5f53\u7136\u5e76\u4e0d\u662f\u8bf4\u6b27\u795e\u662f\u9519\u7684\u3002\u56e0\u4e3a\u6b27\u795e\u662f\u623f\u4ea7\u6295\u8d44\u9886\u57df\u7684\u82e6\u4fee\u6d3e\uff0c\u4ed6\u4e0d\u4e3a\u4efb\u4f55\u6ea2\u4ef7\u4ed8\u8d39\u3002\u8fd9\u5f53\u7136\u662f\u6700\u9ad8\u8d28\u91cf\u7684\u6295\u8d44\u65b9\u5f0f\u3002\u4f46\u662