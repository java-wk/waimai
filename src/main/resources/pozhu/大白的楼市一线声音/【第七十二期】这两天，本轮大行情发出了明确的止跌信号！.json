u653f\u7b56\u3002\u5f88\u591a\u4eba\u90fd\u95ee\uff0c\u8fd9\u9884\u793a\u7740\u4ec0\u4e48\u5462\uff1f\u662f\u4e0d\u662f\u9884\u793a\u7740\u653f\u7b56\u9762\u5df2\u7ecf\u6389\u5934\u4e86\u5417\uff1f\u6211\u7684\u89c2\u70b9\u662f\u83cf\u6cfd\u8fd9\u4e2a\u653f\u7b56\u5bf9\u5e02\u573a\u7684\u5f71\u54cd\u529b\u53ef\u4ee5\u5ffd\u7565\u4e0d\u8bb0\uff0c\u4f46\u662f\u8fd9\u4e2a\u653f\u7b56\u8868\u793a\u9ad8\u5c42\u5bf9\u4e8e\u623f\u4ef7\u4e0a\u6da8\u7684\u5e72\u9884\u5728\u7f29\u5c0f\uff0c\u6240\u4ee5\u8fd9\u662f\u6b62\u8dcc\u4fe1\u53f7\uff0c\u4f46\u8ddd\u79bb\u653f\u7b56\u6389\u5934\u8f6c\u5411\u8fd8\u65e9\u7740\u5462\u3002\u8fd9\u53ea\u662f\u7b2c\u4e00\u9636\u6bb5\uff0c\u5fc3\u7406\u51c6\u5907\u671f\uff0c\u4e3b\u8981\u76ee\u7684\u5c31\u662f\u662f\u5fc3\u7406\u671f\u671b\u503c\u6b62\u8dcc\u3002\u7b2c\u4e8c\u9636\u6bb5\uff0c\u5c31\u662f\u4e00\u57ce\u4e00\u7b56\uff0c\u56e0\u57ce\u65bd\u7b56\uff0c\u5168\u9762\u5f00\u59cb\u3002\u5404\u4e2a\u57ce\u5e02\u6839\u636e\u81ea\u5df1\u6240\u5728\u57ce\u5e02\u5e02\u573a\u884c\u60c5\u9881\u5e03\u5404\u79cd\u653f\u7b56\u3002\u6bd4\u5982\u6b66\u6c49\uff0c12\u67081\u65e5\uff0c\u6b66\u6c49\u5f00\u59cb\u5b9e\u65bd\u65b0\u7248\u4f4f\u623f\u516c\u79ef\u91d1\u5f02\u5730\u8d37\u6b3e\u65b0\u653f\uff1b\u73e0\u6d77\uff0c12\u670819\u65e5\u91d1\u6e7e\u3001\u6597\u95e8\u7684\u9650\u8d2d\u95e8\u69db\u513f\u653e\u677e\uff1b\u6bd4\u598212\u670820\u65e5\uff0c\u5927\u8fde\u90e8\u5206\u94f6\u884c\u4e0b\u8c03\u9996\u5957\u623f\u8d37\u6b3e\u5229\u7387\uff1b\u4f46\u662f\u6211\u4eec\u8bf4\u8fd9\u4e9b\u4fe1\u606f\u4e5f\u90fd\u57fa\u672c\u4e0a\u53ef\u4ee5\u5ffd\u7565\u6389\u7684\u3002\u4e3a\u4ec0\u4e48\u5462\uff1f\u56e0\u4e3a\u8001\u5927\u6ca1\u53d1\u8bdd\uff0c\u8c01\u8bf4\u4e86\u90fd\u4e0d\u7b97\u3002\u4e3a\u4ec0\u4e48\u8fd9\u4e48\u8bf4\u5462\uff1f\u56e0\u4e3a\u4e2d\u56fd\u7684\u57ce\u5e02\u662f\u5206\u7b49\u7ea7\u7684\u3002\u4e00\u7ebf\u3001\u4e8c\u7ebf\u548c\u4e09\u56db\u4e94\u7ebf\u3002\u4e00\u7ebf\u662f\u653f\u7b56\u7684\u65b9\u5411\u6807\uff0c\u53ea\u6709\u4e00\u7ebf\u57ce\u5e02\u6709\u5b9e\u8d28\u6027\u7684\u8f6c\u5411\uff0c\u624d\u80fd\u8bf4\u4e2d\u56fd\u623f\u5730\u4ea7\u8c03\u63a7\u6389\u5934\u8f6c\u5411\u3002\u4e8c\u7ebf\u57ce\u5e02\u5927\u591a\u6570\u7684\u8ffd\u968f\u8005\uff0c\u4e3b\u8981\u770b\u4e00\u7ebf\u57ce\u5e02\uff0c\u89c1\u673a\u884c\u4e8b\u3002\u4e09\u56db\u4e94\u90fd\u662f\u4e00\u4e8c\u7ebf\u57ce\u5e02\u7684\u8d2d\u4e70\u529b\u6ea2\u51fa\u7684\u53d7\u76ca\u8005\u3002\u6211\u4eec\u572811\u6708\u521d\u505a\u8fc7\u4e00\u6b21\u76f4\u64ad\uff0c\u6709\u4e00\u4e2a\u7ed3\u8bba\u662f\u5bf9\u4e8e\u4e2d\u56fd\u7edd\u5927\u591a\u6570\u70ed\u70b9\u4e00\u4e8c\u7ebf\u57ce\u5e02\u800c\u8a00\uff0c\u653f\u7b56\u5e95\u5df2\u7ecf\u51fa\u6765\u4e86\uff0c\u4ef7\u683c\u5e95\u8fd8\u6709\u4e00\u70b9\u70b9\u4e0b\u63a2\u7684\u7a7a\u95f4\u3002\u5f53\u65f6\u4e5f\u5bf9\u63a5\u4e0b\u6765\u697c\u5e02\u653f\u7b56\u8c03\u6574\u6709\u4e00\u4e2a\u9884\u6d4b\uff0c\u4e00\u4e2a\u6708\u540e\u518d\u770b\uff0c\u662f\u975e\u5e38\u51c6\u786e\u7684\u3002\u6211\u4eec\u8ba4\u4e3a1\u3001\u4e00\u7ebf\u9650\u8d2d\u5c06\u957f\u671f\u5b58\u5728\u3002\u4f46\u4f1a\u6709\u6697\u4e2d\u653e\u677e\u7684\u65b9\u5f0f\u3002\u6240\u4ee52019\u5e74\u5b9e\u4f53\u7ecf\u6d4e\u6570\u636e\u5dee\u7684\u8bdd\uff0c\u6216\u8bb8\u6700\u5feb\u660e\u5e74\u4e0b\u534a\u5e74\u4e00\u7ebf\u4f1a\u6709\u66f4\u591a\u7684\u6697\u81ea\u653e\u677e\u7684\u624b\u6bb5\u3002\u5982\u679c\u7ecf\u6d4e\u5f62\u52bf\u597d\u7684\u7684\u8bdd\u6709\u53ef\u80fd\u4f1a\u665a\u534a\u5e74\u6216\u4e00\u5e74\u30022\u3001\u4e8c\u7ebf\u57ce\u5e02\u76842019\u5e74\u5e74\u4e2d\u4f1a\u6162\u6162\u53d6\u6d88\u9650\u8d2d\u3002\u5728\u9650\u8d2d\u53d6\u6d88\u4e4b\u524d\uff0c\u4e8c\u7ebf\u57ce\u5e02\u6697\u4e2d\u653e\u677e\u7684\u624b\u6bb5\u4e5f\u5f88\u591a\uff0c\u6bd4\u5982\u676d\u5dde\u3001\u53a6\u95e8\u3001\u5e7f\u5dde\u964d\u4f4e\u4e86\u6237\u53e3\u7ba1\u5236\u6761\u4ef6\uff0c\u4ee5\u53ca12\u6708\u4efd\u8ddf\u8fdb\u7684\u73e0\u6d77\u3001\u6b66\u6c49\u3002\u4f46\u662f\u66f4\u591a\u4e8c\u7ebf\u57ce\u5e02\u8ddf\u8fdb\uff0c\u9010\u6b65\u51fa\u53f0\u529b\u5ea6\u66f4\u5927\u7684\u653e\u677e\u653f\u7b56\u5927\u6982\u73872019\u5e74\u4e0b\u534a\u5e74\u4e86\u30023\u3001\u9650\u8d37\u4e0d\u4f1a\u8f7b\u6613\u653e\u677e\u7684\u3002\u56e0\u4e3a\u9650\u8d2d\u7684\u653e\u677e\u53ea\u80fd\u8ba9\u623f\u4ef7\u6709\u5c0f\u884c\u60c5\u3002\u4f46\u5982\u679c\u653e\u677e\u9650\u8d37\uff0c\u90a3\u5c31\u662f\u4e00\u8f6e\u4e0a\u6da8\u7684\u5927\u884c\u60c5\u4e86\u91cd\u8981\u4fe1\u53f7\u4e86\u3002\u9664\u975e2019\u5e74\u7ecf\u6d4e\u5f62\u52bf\u975e\u5e38\u4e0d\u4e50\u89c2\uff0c\u5426\u80052019\u5e74\u7edd\u4e0d\u4f1a\u653e\u677e\u9650\u8d37\u7684\u3002\u4f46\u662f\u6211\u4eec\u4e0d\u80fd\u7b49\u5230\u8fd9\u4e48\u660e\u663e\u7684\u4fe1\u53f7\u51fa\u6765\uff0c\u56e0\u4e3a\u5f80\u5f80\u5230\u4e86\u8fd9\u4e2a\u65f6\u5019\uff0c\u6ce2\u6f9c\u58ee\u9614\u7684\u7684\u65f6\u95f4\u5f88\u591a\uff0c\u4e5f\u5c316\u4e2a\u6708\uff0c\u65f6\u95f4\u592a\u77ed\uff0c\u7b49\u5f00\u59cb\u9009\u7b79\u5efa\u4ed3\u9ec4\u82b1\u83dc\u90fd\u51c9\u4e86\u3002\u6240\u4ee5\u6211\u4eec\u9700\u8981\u6301\u7eed\u5173\u6ce8\u660e\u66f4\u591a\u7684\u5e02\u573a\u4fe1\u53f7\uff0c\u9700\u8981\u6301\u7eed\u7684\u5173\u6ce8\u4e00\u3001\u4e8c\u7ebf\u57ce\u5e02\u7684\u4e8c\u624b\u623f\u6210\u4ea4\u91cf\u4ee5\u53ca\u660e\u5e74\u5e74\u521d\u4e24\u4f1a\u7684\u8bb2\u8bdd\u3002\u4ee5\u4e0b10\u6761\u662f\u6211\u4eec\u603b\u7ed3\u51fa\u6765\u7684\u884c\u60c5\u542f\u52a8\u8fdb\u5ea6\u8868\uff0c\u5927\u5bb6\u53ef\u4ee5\u81ea\u884c\u5bf9\u7167\u4e00\u4e0b\uff1a1\uff09\u94f6\u884c\u4fe1\u8d37\u4ea7\u54c1\u5b9e\u9645\u5229\u7387\u7684\u53d8\u53162\uff09\u4fe1\u8d37\u5458\u6709\u6ca1\u6709\u4e3b\u52a8\u8054\u7cfb\u4f60\uff0c\u544a\u77e5\u6709\u66f4\u591a\u7684\u4e1a\u52a13\uff09\u94f6\u884c\u4fe1\u8d37\u4ea7\u54c1\u7684\u53d8\u53164\uff09\u91d1\u878d\u5408\u89c4\u6027\u30025\uff09\u5e02\u573a\u4ef7\u683c\u6ca1\u6709\u6ce2\u52a8\uff0c\u53ef\u662f\u7b0b\u76d8\u6d88\u5931\u901f\u5ea6\u660e\u663e\u52a0\u5feb6\uff09\u5f00\u59cb\u51fa\u73b0\u5c0f\u5e45\u8df3\u4ef7\uff0c\u8ffd\u4ef7\u7684\u884c\u4e3a7\uff09\u5e02\u5185\u5730\u738b\uff0c\u4e00\u624b\u76d8\u5f00\u51fa\u70ed\u70b9\u884c\u60c58\uff09\u9650\u8d2d\u9650\u8d37\u9650\u4ef7\u9650\u552e\u7b49\u653f\u7b56\uff0c\u51fa\u73b0\u5b9e\u8d28\u6027\u677e\u52a8\u30029\uff09\u8206\u8bba\u53e3\u5f00\u59cb\u5bf9\u623f\u5730\u4ea7\u6709\u522910\uff09\u670b\u53cb\u5708\u5f00\u59cb\u5bf9\u623f\u5730\u4ea7\u6709\u5229\u5176\u4e2d\uff0c\u51fa\u73b04\uff0c\u57fa\u672c\u53ef\u4ee5\u786e\u5b9a\u662f\u5927\u653e\u6c34\u524d\u7684\u4fe1\u53f7\u3002\u800c\u51fa\u73b05\uff0c\u5219\u662f\u4e0b\u4e00\u8f6e\u725b\u521d\u7684\u4fe1\u53f7\u3002\u52308 \u5c31\u662f\u4e00\u6ce2\u5927\u884c\u60c5\u786e\u5b9a\u65e0\u7591\u4e86\u3002\u53c2\u7167\u8fd9\u4e2a\u8fdb\u5ea6\u8868\uff0c\u6211\u4eec\u770b\u76ee\u524d\u6240\u6709\u91ca\u653e\u7684\u5229\u597d\u4fe1\u606f\u90fd\u57fa\u672c\u4e0a\u4e0d\u91cd\u8981\uff0c\u4fe1\u53f7\u4e0d\u591f\u5f3a\u70c8\uff0c\u53ea\u80fd\u4ee3\u8868\u8d8b\u52bf\u3002\u6bd4\u5982\u4eca\u5e74\u6240\u6709\u592e\u884c\u7684\u964d\u51c6\u653e\u6c34\uff0c\u8bda\u610f\u662f\u4e0d\u8db3\u7684\u3002\u60f3\u60f3\u770b\u572809\u5e74\u6551\u5e02\u7684\u65f6\u5019\uff0c\u6709\u964d\u51c65%\u7684\u7684\u5386\u53f2\u3002\u6211\u4eec\u8bf4\u771f\u6b63\u80fd\u591f\u5f15\u7206\u5927\u884c\u60c5\u7684\uff0c\u9700\u8981\u7684\u662f\u4e00\u4e2a\u5951\u673a\uff0c\u9700\u8981\u8d70\u5230\u7b2c\u516b\u6761\u3002\u6240\u4ee5\u73b0\u5728\u8bf4\u65b0\u4e00\u8f6e\u7684\u542f\u52a8\u4e3a\u65f6\u5c1a\u65e9\uff0c\u4f46\u662f\u5927\u65b9\u5411\u662f\u4e0d\u53d8\u7684\uff0c2020\u5e74\u662f\u4e00\u5b9a\u4f1a\u5927\u653e\u6c34\u7684\u3002\u8fd9\u5c31\u662f\u6211\u4eec\u5bf9\u672a\u6765\u5e02\u573a\u7684\u9884\u6d4b\u7684\u57fa\u7840\u3002\u53e6\u5916\u4e5f\u501f\u7740\u4eca\u5929\u8bf4\u83cf\u6cfd\u7684\u653f\u7b56\uff0c\u6211\u4eec\u7b80\u5355\u8bf4\u4e00\u4e0b\u4e09\u56db\u4e94\u7ebf\u5c0f\u57ce\u5e02\u5c0f\u53bf\u57ce\u7684\u697c\u5e02\uff0c\u6211\u7684\u89c2\u70b9\u662f\u8fd9\u4e2a\u5730\u65b9\u7684\u697c\u5e02\u6700\u591a\u518d\u6491\u5341\u5e74\uff0c\u53bf\u57ce\u4f30\u8ba1\u4e5f\u5c31\u6491\u4e94\u5e74\uff0c\u6240\u4ee5\u5728\u8fd9\u4e9b\u57ce\u5e02\u6709\u623f\u4ea7\u7684\uff0c\u63a5\u4e0b\u6765\u8fd8\u6709\u4e00\u6b21\u6700\u4f73\u7684\u5356\u51fa\u65f6\u673a\uff0c\u4e00\u5b9a\u8981\u628a\u63e1\u597d\u3002\u5982\u679c\u8fd8\u60f3\u7ee7\u7eed\u4e70\u7684\uff0c\u4e5f\u4e0d\u662f\u4e0d\u884c\uff0c\u539f\u5219\u662f\u5c3d\u91cf\u4e70\u4e00\u5957\u81ea\u4f4f\u5c31\u50cf\uff0c\u5176\u5b83\u7684\u80fd\u51fa\u90fd\u51fa\u5427\u3002\u4e3a\u4e86\u8ba9\u5927\u5bb6\u66f4\u597d\u7406\u89e3\uff0c\u6211\u4eec\u5c31\u4e3e\u4e2a\u6781\u7aef\u7684\u4f8b\u5b50\uff0c\u6bd4\u5982\u53bf\u57ce\u7684\u697c\u5e02\u3002\u6211\u4eec\u8981\u770b\u61c2\u53bf\u57ce\u697c\u5e02\uff0c\u5c31\u5fc5\u987b\u7814\u7a76\u53bf\u57ce\u623f\u5b50\uff0c\u8c01\u5728\u4e70\uff1f1\u3001 \u672c\u53bf\u519c\u6751\u5e74\u8f7b\u4eba\u53bf\u57ce\u4e70\u5a5a\u623f\u5728\u8001\u5bb6\u70ab\u8000\u7528\u30022\u3001 \u672c\u53bf\u519c\u6751\u504f\u8fdc\u6751\u5e84\uff0c\u7ecf\u6d4e\u4e00\u822c\uff0c\u5728\u53bf\u57ce\u4e70\u623f\u517b\u8001\u30023\u3001 \u53bf\u57ce\u571f\u8457\u548c\u4f53\u5236\u5185\u5bb6\u5ead\uff0c\u57fa\u672c\u6709\u62c6\u8fc1\u673a\u4f1a\uff0c\u4e0d\u7b97\u5916\u6765\u65b0\u589e\u9700\u6c42\u3002\u4e5f\u5c31\u662f\u76ee\u524d\u53bf\u57ce\u7684\u8d2d\u4e70\u4e3b\u529b\u65f6\u519c\u675170\u540e\u300180\u540e\uff0c\u53ef\u662f\u8fd9\u4e00\u62e8\u4eba\u80fd\u4e70\u7684\u57fa\u672c\u4e0a\u90fd\u4e70\u4e86\u300290\u540e\u666e\u904d\u4e0d\u613f\u610f\u518d\u56de\u53bf\u57ce\u4e86\uff0c\u800c\u53bf\u57ce\u4f53\u5236\u5185\u5bb6\u5ead\uff0c\u57fa\u672c\u5e0c\u671b\u5b69\u5b50\u53bb\u5927\u57ce\u5e02\u3002\u6240\u4ee5\uff0c\u672a\u67655-10\u5e74\uff0c\u53bf\u57ce\u697c\u5e02\u968f\u7740\u5e74\u8f7b\u4eba\u7684\u7684\u79bb\u5f00\uff0c\u8001\u65e7\u5c0f\u533a\u7684\u7a7a\u7f6e\u623f\u4f1a\u8d8a\u6765\u8d8a\u591a\uff0c\u4e8c\u624b\u5b8c\u5168\u6ca1\u6709\u4ea4\u6613\uff0c\u5c31\u5982\u540c\u4eca\u5929\u7